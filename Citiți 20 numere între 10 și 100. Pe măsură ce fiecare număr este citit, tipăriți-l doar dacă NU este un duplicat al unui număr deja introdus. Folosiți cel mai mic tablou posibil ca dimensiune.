<body>
<script type="text/javascript">
var even=0, n, odd=1;
n=prompt("Introduce un numar");
while(even<n)
{
even=even+2;
document.write(even +"<br>");
}
document.write("old no are "+"<br>");
while(odd<n)
{
document.write(odd+"<br>");
odd=odd+2;
}
</script>
